{
    "name": "babelconfig",
    "version": "1.0.0",
    "description": "npm i -D typescript @babel/core @babel/preset-env @babel/preset-typescript",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "tsc -w",
        "build:types": "tsc",
        "build:application": "babel src/ --out-dir dist/ --extensions \".ts\"",
        "build": "yarn build:types && yarn build:application"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.15",
        "@babel/node": "^7.13.13",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-typescript": "^7.13.0",
        "typescript": "^4.2.4"
    }
}